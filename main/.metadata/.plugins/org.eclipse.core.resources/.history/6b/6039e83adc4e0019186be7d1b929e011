package com.capgemini.files.model;

public class Employee {

	private int empId;
	private String name;
	private double salary;
	
	
	public Employee() {
		super();
		
	}


	public Employee(int empId, String name, double salary) {
		super();
		this.empId = empId;
		this.name = name;
		this.salary = salary;
	}
	
	
	
}
