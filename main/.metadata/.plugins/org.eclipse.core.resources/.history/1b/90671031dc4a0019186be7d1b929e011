package com.capgemini.student.model;

public class Student {
	
	private String name;
	private int rollNo;
	private int marks;
	
	public Student() {
		super();
		
	}

	public Student(String name, int rollNo, int marks) {
		super();
		this.name = name;
		this.rollNo = rollNo;
		this.marks = marks;
	}
	

	
	
	
	

}
